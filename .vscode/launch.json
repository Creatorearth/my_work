      "MIMode": "gdb",
      "targetArchitecture": "arm",
      "miDebuggerPath": "",
      "debugServerPath": "",
      "debugServerArgs": "",
      "customLaunchSetupCommands": [
        {
          "text": "target remote localhost:3333"
        },
        {
          "text": "file \"${file}\""
        },
        {
          "text": "load"
        },
        {
          "text": "monitor reset halt"
        },
        {
          "text": "monitor reset init"
        }
      ],
      "stopAtEntry": true,
      "serverStarted": "Info\\ :\\ [\\w\\d\\.]*:\\ hardware",
